            //    alert("O campo NOME deve ser preenchido com no minimo 5 letras");
            //    return false;
            //}
            //return true;
        }

        function validaEstado(estado) {
            $("#userEstadoSelect option:selected").prop("selected", false);
            $("#userEstadoSelect option[id='" + estado + "']").prop("selected", true);
            carregaCidades(estado);
        }

        function carregaEstados() {
            var serviceURL = '/Home/GetEstados';

            $.ajax({
                type: "POST",
                url: serviceURL,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data, status) {
                    console.log(data);
                    for (var i = 0; i < data.length; i++) {
                        $("#userEstadoSelect").append("<option id='" + data[i].UF + "'>" + data[i].Name + "</option>");
                    }
                },
                error: errorFunc
            });

        }

        function carregaCidades(ufParam) {
            $("#userCidadeSelect option:not(:contains('noSelect'))").remove();
            var uf = { uf: ufParam };
            var serviceURL = '/Home/GetCidades';
            $.ajax({
                type: "POST",
                url: serviceURL,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: JSON.stringify(uf),
                success: function (data, status) {
                    console.log(data);
                    for (var i = 0; i < data.length; i++) {
                        $("#userCidadeSelect").append("<option id='" + data[i].Nome + "'>" + data[i].Nome + "</option>");
                    }
                    $("#userCidadeSelect option[id='" + cidade + "']").prop("selected", true);
                },
                error: errorFunc
            });

        }

