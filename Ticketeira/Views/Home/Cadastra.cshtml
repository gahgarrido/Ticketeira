        function validaLogin(objUser, passwordConfirmation) {
            if (objUser.getPassword() != passwordConfirmation) {
                alert("Password de Confirmação Inválido!");
                return false;
            }   
            return true;
        }

        function validaUsuario(objUser) {
            if (objUser.getUser() == "" || objUser.getUser().length < 5) {
                alert("O campo USUÁRIO deve ser preenchido com no minimo 5 letras");
                return false;
            }
            return true;
        }

        function validaPassword(objUser) {
            if (objUser.getPassword() == "" || objUser.getPassword().length < 5) {
                alert("O campo SENHA deve ser preenchido com no minimo 5 letras");
                return false;
            }
            return true;
        }

        function validaName(objUser) {
            if (objUser.getName() == "" || objUser.getName().length < 5) {
                alert("O campo NOME deve ser preenchido com no minimo 5 letras");
                return false;
            }
            return true;
        }

