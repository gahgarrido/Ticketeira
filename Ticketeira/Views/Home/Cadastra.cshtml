    <script type="text/javascript">
        var cidade = "";
        $(document).ready(function () {
            carregaEstados();

            $("#userCep").keyup(function () {
                if ($("#userCep").val().length >= 8) {
                    var teste = "https://viacep.com.br/ws/" + $("#userCep").val() + "/json/";

                    $.ajax({
                        type: "GET",
                        url: teste,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data, status) {
                            $("#UserAddress").val(data.logradouro);
                            validaEstado(data.uf);
                            cidade = data.localidade;
                        }

                    });
                }
            });

            $("#userEstadoSelect").change(function () {
                carregaCidades($("#userEstadoSelect option:selected").attr('id'));
            });

            $("#btnCadastrar").click(function () {
                var usuarioObj = ObjUsuario();

                usuarioObj.setUser($("#user").val());
                usuarioObj.setPassword($("#userPassword").val());
                usuarioObj.setName($("#userName").val());
                usuarioObj.setEmail($("#userEmail").val());
                usuarioObj.setAddress($("#UserAddress").val());
                usuarioObj.setNumber($("#userNumber").val());
                usuarioObj.setAddress2($("#userAddress2").val());
                usuarioObj.setCep($("#userCep").val());
